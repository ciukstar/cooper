<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <navigation-rule>
        <from-view-id>/secure/status/status-list.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>status-edit</from-outcome>
            <to-view-id>/secure/status/status-edit.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/secure/status/status-list.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>graph-list</from-outcome>
            <to-view-id>/secure/graph/graph-list.xhtml</to-view-id>
            <redirect />
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/secure/admin/user-list.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>graph-list</from-outcome>
            <to-view-id>/secure/status/graph-list.xhtml</to-view-id>
            <redirect />
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/secure/status/graph-list.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>status-list</from-outcome>
            <to-view-id>/secure/status/status-list.xhtml</to-view-id>
            <redirect />
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/secure/admin/user-list.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>status-list</from-outcome>
            <to-view-id>/secure/status/status-list.xhtml</to-view-id>
            <redirect />
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/secure/admin/user-list.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>user-edit</from-outcome>
            <to-view-id>/secure/admin/user-edit.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>user-list</from-outcome>
            <to-view-id>/secure/admin/user-list.xhtml</to-view-id>
            <redirect />
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>home</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
            <redirect />
        </navigation-case>
    </navigation-rule>
</faces-config>
